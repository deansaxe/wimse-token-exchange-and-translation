      "reviews": [
        {
          "id": "PRR_kwDOLqam7s6APb7r",
          "commit": {
            "abbreviatedOid": "ba549fe"
          },
          "author": "gffletch",
          "authorAssociation": "COLLABORATOR",
          "state": "CHANGES_REQUESTED",
          "body": "I fixed this issue in my PR. We can delete this one :)",
          "createdAt": "2024-07-01T14:20:59Z",
